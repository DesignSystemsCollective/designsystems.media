{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": "'media'",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "tags",
          "type": "tags"
        },
        {
          "name": "speakers",
          "type": "taxonomy",
          "taxonomyId": "speakers"
        },
        {
          "name": "draft",
          "type": "draft",
          "required": true
        },
        {
          "name": "description",
          "type": "string",
          "single": true
        },
        {
          "name": "publishedAt",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "dateAdded",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "name": "image",
          "type": "image"
        },
        {
          "name": "poster",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "name": "localImages",
          "type": "boolean",
          "required": true
        },
        {
          "name": "duration",
          "type": "string",
          "single": true
        },
        {
          "name": "privacyStatus",
          "type": "string",
          "single": true
        },
        {
          "name": "videoUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "categories",
          "type": "categories"
        }
      ]
    },
    {
      "name": "podcast",
      "previewPath": "'podcast'",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "publishedAt",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "dateAdded",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "name": "episodeUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "audioUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "podcastTitle",
          "type": "string",
          "single": true
        },
        {
          "name": "showSlug",
          "type": "string",
          "single": true
        },
        {
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "name": "localImages",
          "type": "boolean",
          "required": true
        },
        {
          "name": "tags",
          "type": "tags"
        },
        {
          "name": "categories",
          "type": "categories"
        },
        {
          "name": "duration",
          "type": "string",
          "single": true
        },
        {
          "name": "durationSeconds",
          "type": "number"
        },
        {
          "name": "draft",
          "type": "draft",
          "required": true
        },
        {
          "name": "speakers",
          "type": "taxonomy",
          "taxonomyId": "speakers"
        },
        {
          "name": "type",
          "type": "string",
          "single": true,
          "default": "podcast"
        },
        {
          "name": "season",
          "type": "number"
        },
        {
          "name": "episode",
          "type": "number"
        },
        {
          "name": "explicit",
          "type": "boolean",
          "default": false
        },
        {
          "name": "feedUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "guid",
          "type": "string",
          "single": true
        },
        {
          "name": "hasEpisodeImage",
          "type": "boolean",
          "default": false
        }
      ]
    },
    {
      "name": "show",
      "previewPath": "'show'",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "description",
          "type": "string",
          "single": true
        },
        {
          "name": "speakers",
          "type": "taxonomy",
          "taxonomyId": "speakers"
        },
        {
          "name": "feedUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "websiteUrl",
          "type": "string",
          "single": true
        },
        {
          "name": "imageReference",
          "type": "string",
          "single": true
        },
        {
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "name": "dateAdded",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "name": "lastUpdate",
          "type": "datetime"
        },
        {
          "name": "categories",
          "type": "categories"
        },
        {
          "name": "language",
          "type": "string",
          "single": true,
          "default": "en-us"
        },
        {
          "name": "explicit",
          "type": "boolean",
          "default": false
        },
        {
          "name": "episodeCount",
          "type": "number"
        },
        {
          "name": "localImages",
          "type": "boolean",
          "required": true
        },
        {
          "name": "itunesId",
          "type": "number"
        },
        {
          "name": "guid",
          "type": "string",
          "single": true
        },
        {
          "name": "medium",
          "type": "string",
          "single": true,
          "default": "podcast"
        },
        {
          "name": "dead",
          "type": "number",
          "default": 0
        },
        {
          "name": "locked",
          "type": "number",
          "default": 0
        },
        {
          "name": "type",
          "type": "string",
          "single": true,
          "default": "show"
        },
        {
          "name": "draft",
          "type": "draft",
          "required": true
        },
        {
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.pageFolders": [
    {
      "title": "media",
      "path": "[[workspace]]/src/content/media"
    },
    {
      "title": "podcast",
      "path": "[[workspace]]/src/content/podcast"
    },
    {
      "title": "show",
      "path": "[[workspace]]/src/content/show"
    }
  ],
  "frontMatter.content.publicFolder": "public",
  "frontMatter.git.enabled": true,
  "frontMatter.content.filters": [
    "contentFolders",
    "tags",
    "categories",
    "speakers"
  ],
  "frontMatter.content.sorting": [
    {
      "title": "Media published (asc)",
      "name": "publishedAt",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Media published (desc)",
      "name": "publishedAt",
      "order": "desc",
      "type": "date"
    }
  ],
